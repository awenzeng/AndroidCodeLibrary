<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.awen.plugin_module">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidCodeLibrary">
        <activity
            android:name=".MainPluginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TopActivity"></activity>

        <receiver android:name=".AwenStaticRegisterReceiver"
            android:exported="true">
            <intent-filter>
                <action android:name="com.awen.pluginmodule.action"></action>
            </intent-filter>
        </receiver>
    </application>

</manifest>